 0002           ;-----------------------------------------------------------------------------
 0002           ;  FUNCTION NAME: E2PROM_1_E2Write
 0002           ;
 0002           ;  DESCRIPTION:
 0002           ;     Writes the specified E2PROM data at the wAddr and wByteCount from RAM into
 0002           ;     Flash into the defined E2PROM instance.
 0002           ;
 0002           ;     Prototype in C is:
 0002           ;
 0002           ;        #pragma  fastcall16 E2Write
 0002           ;        void E2PROM_1_bE2Write( WORD wAddr, BYTE * pbData, WORD wByteCount,
 0002           ;                                        CHAR cTemperature );
 0002           ;
 0002           ;     Easiest method to call from assembly is as follows:
 0002           ;           push  X                    ; push X only if X needs to be perserved
 0002           ;           mov   X, SP
 0002           ;           push  <cTemperature>
 0002           ;           push  <wByteCount>
 0002           ;           push  <wByteCount+1>
 0002           ;           push  <pbDataDest>                  
 0002           ;           push  <pbDataDest+1>
 0002           ;           push  <wAddr>
 0002           ;           push  <wAddr+1>
 0002           ;           call  E2PROM_1_bE2Write
 0002           ;           add   SP, -E2_RD_ARG_STACK_FRAME_SIZE
 0002           ;           pop   X                    ; restore X only if it was saved above
 0002           ;
 0002           ;           where <> refers to any addressing mode or number of instructions to
 0002           ;              place the referenced data on the stack frame.
 0002           ;
 0002           ;     Other method is to create a stack frame using the defined equates and
 0002           ;     load them into the stack frame using [X+E2_WR_ARG_*] offset addressing mode.
 0002           ;     If this method is used in the LMM (large memory model) then make sure
 0002           ;     that the index page tracks the stack page or is set to the same page
 0002           ;     as the stack page.
 0002           ;
 0002           ;-----------------------------------------------------------------------------
 0002           ;
 0002           ;  ARGUMENTS:
 0002           ;     wAddr:         WORD   - relative OFFSET in defined E2PROM to write data
 0002           ;     pbData:        BYTE * - pointer to the RAM buffer of data to write
 0002           ;     wByteCount:    WORD   - number of bytes to write into E2PROM
 0002           ;     cTemperature:  CHAR   - temperature in degrees celsius
 0002           ;
 0002           ;  RETURNS:    E2PROM_1_NOERROR, E2PROM_1_FAILURE, or
 0002           ;              E2PROM_1_STACKOVERFLOW
 0002           ;
 0002           ;  SIDE EFFECTS: 
 0002           ;    If a partial block is to be saved to flash, then a 64 byte buffer
 0002           ;    is temporary allocated.
 0002           ;
 0002           ;    The A and X registers may be modified by this or future implementations
 0002           ;    of this function.  The same is true for all RAM page pointer registers in
 0002           ;    the Large Memory Model.  When necessary, it is the calling function's
 0002           ;    responsibility to perserve their values across calls to fastcall16 
 0002           ;    functions.
 0002           ;          
 0002           ;    Currently only the page pointer registers listed below are modified:
 0002           ;       CUR_PP 
 0002           ;       IDX_PP
 0002           ;       MVW_PP
 0002           ;                
 0002           ;     REGISTERS ARE VOLATILE: THE A AND X REGISTERS MAY BE MODIFIED!
 0002           ;------------------------------------------------------------------------------
 0002           .SECTION
 0002           
 0002            E2PROM_1_bE2Write::
 0002           _E2PROM_1_bE2Write::
 0002           
 0002           ;  RAM_PROLOGUE RAM_PROXY_CLASS_4 RAM_USE_CLASS_2  -->  implemented in E2PROMLIB  
 0002           ;  Save the Flag register to perserve the native paging mode
                IF (CPU_F)     ;only save CPU flag if NOT CY8C25/26xxx PSoC devices
 0002 5DF7         mov   A, reg[CPU_F]                            
 0004 08           push  A
                ENDIF
 0005 5000         mov   A, >E2PROM_1_START_BLOCK          ; push the FirstBlockID - MSB
 0007 08           push  A
 0008 507F         mov   A, <E2PROM_1_START_BLOCK          ; push the FirstBlockID - LSB
 000A 08           push  A
 000B 7D0000       ljmp  bE2Write                                  ; jmp to E2PROMLIB 
 000E           
 000E           
 000E           ; NOTE that return will be done by a RETI instruction to restore the orginal paging
 000E           ;      mode and flag status.
 000E           
 000E           .ENDSECTION
 000E           
 000E           
 000E           ;-----------------------------------------------------------------------------
 000E           ;  FUNCTION NAME: E2PROM_1_E2Read
 000E           ;
 000E           ;  DESCRIPTION:
 000E           ;     Reads the specified E2PROM data at offset=wAddr for wByteCount bytes  and
 000E           ;     places the data read into the RAM buffer pbDataDest.
 000E           ;
 000E           ;     Prototype in C is:
 000E           ;
 000E           ;        #pragma  fastcall16 E2Read
 000E           ;        void E2PROM_1_E2Read( WORD wAddr, BYTE * pbDataDest, WORD wByteCount );
 000E           ;
 000E           ;     Easiest method to call from assembly is as follows:
 000E           ;           push  X                             ; only if X needs to be preserved
 000E           ;           mov   X, SP
 000E           ;           push  <wByteCount>
 000E           ;           push  <wByteCount+1>
 000E           ;           push  <pbDataDest
 000E           ;           push  <pbDataDest+1>
 000E           ;           push  <wAddr>
 000E           ;           push  <wAddr+1>
 000E           ;           call  E2PROM_1_E2Read
 000E           ;           add   SP, -E2_RD_ARG_STACK_FRAME_SIZE
 000E           ;           pop   X                             ; restore only if X needs to be preserved
 000E           ;
 000E           ;           where <> refers to any addressing mode or number of instructions to
 000E           ;              place the referenced data on the stack frame.
 000E           ;
 000E           ;     Other method is to create a stack frame using the defined equates and
 000E           ;     load them into the stack frame using [X+E2_WR_ARG_*] offset addressing mode.
 000E           ;     If this method is used in the LMM (large memory model) then make sure
 000E           ;     that the index page tracks the stack page or is set to the same page
 000E           ;     as the stack page.
 000E           ;
 000E           ;-----------------------------------------------------------------------------
 000E           ;
 000E           ;  ARGUMENTS:
 000E           ;     wAddr:         WORD   - relative OFFSET in defined E2PROM to read data
 000E           ;     pbDataDest:    BYTE * - pointer to the RAM buffer to place read data
 000E           ;     wByteCount:    WORD   - number of bytes to read from E2PROM
 000E           ;
 000E           ;  RETURNS:       none
 000E           ;
 000E           ;  SIDE EFFECTS:
 000E           ;    REGISTERS ARE VOLATILE: THE A AND X REGISTERS MAY BE MODIFIED 
 000E           ;    AS MAY THE PAGE POINTER REGISTERS! 
 000E           ;    
 000E           ;    In the large memory model currently only the page
 000E           ;    pointer registers listed below are modified.  This does
 000E           ;    not guarantee that in future implementations of this
 000E           ;    function other page pointer registers will not be
 000E           ;    modified.
 000E           ;    
 000E           ;    Page Pointer Registers Modified:
 000E           ;          CUR_PP
 000E           ;          MVW_PP 
 000E           ;
 000E           ;-----------------------------------------------------------------------------
 000E           .SECTION
 000E           
 000E            E2PROM_1_E2Read::
 000E           _E2PROM_1_E2Read::
 000E           ;  RAM_PROLOGUE RAM_PROXY_CLASS_NULL RAM_USE_CLASS_4  -->  implemented in E2PROMLIB  
 000E           ;  Save the Flag register to perserve the native paging mode
                IF (CPU_F)     ;only save CPU flag if NOT CY8C25/26xxx PSoC devices
 000E 5DF7         mov   A, reg[CPU_F]                            
 0010 08           push  A
                ENDIF
                
                ;  Load the Block ID into X and A
 0011 5000         mov   A, >E2PROM_1_START_BLOCK       
 0013 5C           mov   X, A
 0014 507F         mov   A, <E2PROM_1_START_BLOCK
 0016 7D0000       ljmp  E2Read
 0019           
 0019           ; NOTE that return will be done by a RETI instruction to restore the orginal paging
 0019           ;      mode and flag status.
 0019           
 0019           
 0019           .ENDSECTION
